<mujoco model="model">
  <asset>
    <texture file="visuals/material_0.png" name="tex-model" type="2d"/>
    <texture type="2d" name="image0" file="visuals/image0.png"/>
    <texture type="2d" name="image1" file="visuals/image1.png"/>
    <material name="model" reflectance="0.5" texrepeat="1 1" texture="tex-model" texuniform="false"/>
    <material name="metal" specular="0.5" shininess="0.055367579" rgba="0.355950 0.355950 0.355950 1.0"/>
    <material name="paperSide" texture="image0" specular="0.5" shininess="0.0"/>
    <material name="paperTopAndBottom" texture="image1" specular="0.5" shininess="0.0"/>
    <material name="paperboard" specular="0.5" shininess="0.0" rgba="0.327778 0.266356 0.198069 1.0"/>
    <mesh file="visuals/model_0.obj" name="model_0_vis" scale="1 1 1"/>
    <mesh file="visuals/model_1.obj" name="model_1_vis" scale="1 1 1"/>
    <mesh file="visuals/model_2.obj" name="model_2_vis" scale="1 1 1"/>
    <mesh file="visuals/model_3.obj" name="model_3_vis" scale="1 1 1"/>
  </asset>
  <default>
    <default class="visual">
      <geom conaffinity="0" contype="0" group="1" type="mesh"/>
    </default>
    <default class="collision">
      <geom group="0" rgba="0.5 0 0 0.5"/>
    </default>
  </default>
  <worldbody>
    <body>
      <!-- exterior bounding box points -->
      <geom name="reg_ext" conaffinity="0" contype="0" group="1" type="box" size="0.2740758035431492 0.2740758378026246 0.5" pos="0.0 0.0 0.0" rgba="0 0 1 0"/>
      <!-- interior bounding box points -->
      <body name="object">
        <geom solimp="0.998 0.998 0.001" solref="0.001 1" density="100" friction="0.95 0.3 0.1" type="mesh" mesh="model_0_vis" material="paperboard" class="visual"/>
        <geom solimp="0.998 0.998 0.001" solref="0.001 1" density="100" friction="0.95 0.3 0.1" type="mesh" mesh="model_1_vis" material="paperTopAndBottom" class="visual"/>
        <geom solimp="0.998 0.998 0.001" solref="0.001 1" density="100" friction="0.95 0.3 0.1" type="mesh" mesh="model_2_vis" material="paperSide" class="visual"/>
        <geom solimp="0.998 0.998 0.001" solref="0.001 1" density="100" friction="0.95 0.3 0.1" type="mesh" mesh="model_3_vis" material="metal" class="visual"/>
        <geom class="collision" type="box" pos="0 0 0" size="0.2740758035431492 0.2740758378026246 0.5"/>
      </body>
    </body>
  </worldbody>
</mujoco>
