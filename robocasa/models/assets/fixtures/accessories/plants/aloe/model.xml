<mujoco model="model">
  <compiler angle="radian"/>
  <asset>
    <texture file="visuals/material_0.png" name="tex-model" type="2d"/>
    <texture type="2d" name="image0" file="visuals/image0.png"/>
    <texture type="2d" name="image1" file="visuals/image1.png"/>
    <texture type="2d" name="image2" file="visuals/image2.png"/>
    <texture type="2d" name="image3" file="visuals/image3.png"/>
    <material name="model" reflectance="0.5" texrepeat="1 1" texture="tex-model" texuniform="false"/>
    <material name="blinn1SG" texture="image0" specular="0.5" shininess="0.15999998099999999"/>
    <material name="blinn2SG" texture="image1" specular="0.5" shininess="0.15999998099999999"/>
    <material name="blinn3SG" texture="image2" specular="0.5" shininess="0.15999998099999999"/>
    <material name="blinn4SG" texture="image3" specular="0.5" shininess="0.0"/>
    <mesh file="visuals/model_0.obj" name="model_0_vis" scale="1 1 1"/>
    <mesh file="visuals/model_1.obj" name="model_1_vis" scale="1 1 1"/>
    <mesh file="visuals/model_2.obj" name="model_2_vis" scale="1 1 1"/>
    <mesh file="visuals/model_3.obj" name="model_3_vis" scale="1 1 1"/>
  </asset>
  <default>
    <default class="visual">
      <geom conaffinity="0" contype="0" group="1" type="mesh"/>
    </default>
    <default class="collision">
      <geom group="0" rgba="0.5 0 0 0.5"/>
    </default>
  </default>
  <worldbody>
    <body>
      <!-- exterior bounding box points -->
      <!-- interior bounding box points -->
      <body name="object">
        <geom solimp="0.998 0.998 0.001" solref="0.001 1" density="100" friction="0.95 0.3 0.1" type="mesh" mesh="model_0_vis" material="blinn2SG" class="visual"/>
        <geom solimp="0.998 0.998 0.001" solref="0.001 1" density="100" friction="0.95 0.3 0.1" type="mesh" mesh="model_1_vis" material="blinn1SG" class="visual"/>
        <geom solimp="0.998 0.998 0.001" solref="0.001 1" density="100" friction="0.95 0.3 0.1" type="mesh" mesh="model_2_vis" material="blinn4SG" class="visual"/>
        <geom solimp="0.998 0.998 0.001" solref="0.001 1" density="100" friction="0.95 0.3 0.1" type="mesh" mesh="model_3_vis" material="blinn3SG" class="visual"/>
        <geom class="collision" type="box" pos="0 0 0" size="0.35 0.35 0.49999999999999994"/>
        <geom name="reg_main_body" conaffinity="0" contype="0" group="1" type="box" size="0.35 0.35 0.49999999999999994" pos="0.0 0.0 0.0" rgba="1 1 0 0"/>
      </body>
    </body>
  </worldbody>
</mujoco>
