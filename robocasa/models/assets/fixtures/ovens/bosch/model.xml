<mujoco model="model">
  <compiler angle="radian"/>
  <asset>
    <texture file="visuals/material_0.png" name="tex-model" type="2d"/>
    <texture type="2d" name="image0" file="visuals/image0.png"/>
    <texture type="2d" name="image3" file="visuals/image3.png"/>
    <material name="model" reflectance="0.5" texrepeat="1 1" texture="tex-model" texuniform="false"/>
    <material name="boxLP" texture="image0" specular="0.5" shininess="0.25"/>
    <material name="doorLP" texture="image3" specular="0.5" shininess="0.25"/>
    <mesh file="visuals/model_0.obj" name="model_0_vis" scale="1 1 1"/>
    <mesh file="visuals/model_1.obj" name="model_1_vis" scale="1 1 1"/>
  </asset>
  <default>
    <default class="visual">
      <geom conaffinity="0" contype="0" group="1" type="mesh"/>
    </default>
    <default class="collision">
      <geom group="0" rgba="0.5 0 0 0.5"/>
    </default>
  </default>
  <worldbody>
    <body>
      <!-- exterior bounding box points -->
      <site rgba="0 0 0 1" size="0.01" pos="-0.5 -0.38 -0.4880809808953521" name="ext_p0"/>
      <site rgba="1 0 0 1" size="0.01" pos="0.5 -0.38 -0.4880809808953521" name="ext_px"/>
      <site rgba="0 1 0 1" size="0.01" pos="-0.5 0.44635753124293276 -0.4880809808953521" name="ext_py"/>
      <site rgba="0 0 1 1" size="0.01" pos="-0.5 -0.38 0.4880809808953521" name="ext_pz"/>
      <!-- interior bounding box points -->
      <body name="object">
        <geom solimp="0.998 0.998 0.001" solref="0.001 1" density="100" friction="0.95 0.3 0.1" type="mesh" mesh="model_0_vis" material="boxLP" class="visual"/>
        <geom solimp="0.998 0.998 0.001" solref="0.001 1" density="100" friction="0.95 0.3 0.1" type="mesh" mesh="model_1_vis" material="doorLP" class="visual"/>
        <geom class="collision" type="box" pos="0 0.03 0" size="0.5 0.41 0.4880809808953521"/>
      </body>
    </body>
  </worldbody>
</mujoco>